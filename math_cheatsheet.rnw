curve(dgamma(x, 0, 0), 0, 2, col="red", add=TRUE)
legend("topright", 
       legend=c("Ga(0.5, 0.5)", "Ga(0,0)"), 
                col=c("black", "red"), lty=1,
                bty="n")
@ 


